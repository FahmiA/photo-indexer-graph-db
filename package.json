{
  "name": "photo-indexer-graph-db",
  "version": "1.0.0",
  "description": "Index a library of photos in Azure",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc --watch",
    "build:production": "npm run prestart && npm prune --production",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host build-watch"
  },
  "dependencies": {
    "dms2dec": "^1.1.0",
    "exif-reader": "^1.0.3",
    "geojson-geometries-lookup": "^0.3.1",
    "gremlin": "^3.4.6",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@azure/functions": "^1.2.0",
    "@types/gremlin": "^3.4.6",
    "@types/node": "^12.12.35",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.8.3"
  }
}
